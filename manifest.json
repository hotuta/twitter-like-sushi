{
	"name": "like2sushi for ChromeExtension",
	"version": "0.6",
	"manifest_version": 2,
	"description": "いいね♥をすしにします。",
	"web_accessible_resources": ["like2sushi.css"],
	"content_scripts": [
		{
			"matches": ["https://twitter.com/*","https://tweetdeck.twitter.com/*"],
			"js": ["sushi_query.js", "like2sushi.js"],
			"css": ["like2sushi.css"]
    }
  ],
	"icons": {
		"128": "icon.png"
	}
}
